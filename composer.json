{
    "name": "wildbit/craft3-donottrack",
    "description": "Craft 3 plugin for respecting Do Not Track headers sent by browsers.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "do not track"
    ],
    "support": {
        "docs": "https://github.com/wildbit/craft3-donottrack/blob/master/README.md",
        "issues": "https://github.com/wildbit/craft3-donottrack/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wildbit",
            "homepage": "https://wildbit.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "wildbit\\donottrack\\": "src/"
        }
    },
    "extra": {
        "name": "Do Not Track",
        "handle": "do-not-track",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/wildbit/craft3-donottrack/master/CHANGELOG.md",
        "components": {
            "doNotTrackService": "wildbit\\donottrack\\services\\DoNotTrackService"
        },
        "class": "wildbit\\donottrack\\DoNotTrack"
    }
}
